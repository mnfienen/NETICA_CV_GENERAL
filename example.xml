<data>
	<control_data>
		<baseNET>glacial4433_bins4_5.neta</baseNET>
		<baseCAS>glacial4433.cas</baseCAS>
		<rebin_flag>True</rebin_flag>
		<originalNET>glacial4433.neta</originalNET>
		<pwdfile>mikeppwd.txt</pwdfile>
	</control_data>
	<kfold_data>
		<CVflag>True</CVflag>
		<numfolds>10</numfolds>
	</kfold_data>

	<scenario>
		<name>glacialbins4_5</name>
		<input>log_TRANGM</input> 
		<input>sqrt_SW_MIN</input> 
		<input>PCT_SW</input> 
		<input>RCHSUM</input> 
		<input>sqrt_RIVMIN1</input> 
		<input>PCTORD1</input>
		<response>EXT_FLOW</response> 
		<response>SW_SRC</response> 
		<response>SRC_ORD1</response> 
		<response>SRC_ORD2</response> 
		<response>SRC_ORD3</response>
	</scenario>	

	<sensitivity>
		<report_sens>True</report_sens>
	</sensitivity>
	<learnCPTdata>
		<voodooPar>100</voodooPar>
		<useEM>True</useEM>
	</learnCPTdata>

	<rebinning>
	<!-- if rebin_flag is True, then bin_setup.py will read in the 
		rebin_name to write out the rebinned .neta file and will
		use the newbins information for that purpose -->
			<newbins>
				<node numbins="4">log_TRANGM</node>
				<node numbins="4">sqrt_SW_MIN</node>
				<node numbins="4">PCT_SW</node>
				<node numbins="4">RCHSUM</node>
				<node numbins="4">sqrt_RIVMIN1</node>
				<node numbins="4">PCTORD1</node>
				<node numbins="5">EXT_FLOW</node>
				<node numbins="5">SW_SRC</node>
				<node numbins="5">SRC_ORD1</node>
				<node numbins="5">SRC_ORD2</node>
				<node numbins="5">SRC_ORD3</node>
			</newbins>
	</rebinning>
	
</data>